    "nock": "^12.0.3",
    "roboto-fontface": "*",
    "vue": "^2.6.11",
    "vue-i18n": "^8.18.2",
    "vue-router": "^3.2.0",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.0.0",
